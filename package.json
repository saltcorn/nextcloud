{
  "name": "@saltcorn/nextcloud",
  "version": "0.1.3",
  "description": "Nextcloud integration",
  "main": "index.js",
  "dependencies": {
    "@saltcorn/markup": "^0.9.1",
    "@saltcorn/data": "^0.9.1",
    "@saltcorn/nctalkclient": "1.6.1-beta.0"
  },
  "author": "Tom Nielsen",
  "license": "MIT",
  "repository": "github:saltcorn/nextcloud",
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
